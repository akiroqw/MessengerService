<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryIconColor" Color="White"/>
    <SolidColorBrush x:Key="IconPressedColor" Color="#a8a5ff"/>
    <SolidColorBrush x:Key="HoverIconColor" Color="Gray"/>

    <Color x:Key="HoverPanelButtonColor">#252525</Color>
    <SolidColorBrush x:Key="HoverControlPanelButtonColorBrush" Color="{StaticResource HoverPanelButtonColor}" />

    <Color x:Key="PressedPanelButtonColor">#151515</Color>
    <SolidColorBrush x:Key="PressedControlPanelButtonColorBrush" Color="{StaticResource PressedPanelButtonColor}" />

    <Color x:Key="HoverCloseButtonColor">#E81123</Color>
    <SolidColorBrush x:Key="HoverCloseButtonColorBrush" Color="{StaticResource HoverCloseButtonColor}" />

    <Color x:Key="PressedCloseButtonColor">#CC1123</Color>
    <SolidColorBrush x:Key="PressedCloseButtonColorBrush" Color="{StaticResource PressedCloseButtonColor}" />

    <Color x:Key="WindowBackgroundColor">#1E1E1E</Color>
    <SolidColorBrush x:Key="WindowBackgroundColorBrush" Color="{StaticResource WindowBackgroundColor}"/>

    <Color x:Key="WindowTitleBarBackgroundColor">#303030</Color>
    <SolidColorBrush x:Key="WindowTitleBarBackgroundColorBrush" Color="{StaticResource WindowTitleBarBackgroundColor}"/>

    <Color x:Key="ButtonColor">#a8a5ff</Color>
    <SolidColorBrush x:Key="ButtonColorBrush" Color="{StaticResource ButtonColor}"/>

    <Color x:Key="ColorWrap">#252525</Color>
    <SolidColorBrush x:Key="ColorWrapBrush" Color="{StaticResource ColorWrap}"/>

    <Color x:Key="HeaderChatColor">#262626</Color>
    <SolidColorBrush x:Key="HeaderChatColorBrush" Color="{StaticResource HeaderChatColor}"/>

    <Color x:Key="BottomChatPanelColor">#262626</Color>
    <SolidColorBrush x:Key="BottomChatPanelColorBrush" Color="{StaticResource BottomChatPanelColor}"/>

    <Color x:Key="ChatTextBoxColor">#1E1E1E</Color>
    <SolidColorBrush x:Key="ChatTextBoxColorBrush" Color="{StaticResource ChatTextBoxColor}"/>

    <Color x:Key="UsersListBackgroundColor">#262626</Color>
    <SolidColorBrush x:Key="UsersListBackgroundColorBrush" Color="{StaticResource UsersListBackgroundColor}"/>

    <Color x:Key="MenuBackgroundColor">#262626</Color>
    <SolidColorBrush x:Key="MenuBackgroundColorColorBrush" Color="{StaticResource MenuBackgroundColor}"/>

    <Color x:Key="ButtonBorderColor">#5E6175</Color>
    <SolidColorBrush x:Key="ButtonBorderBrushColor" Color="{StaticResource ButtonBorderColor}"/>

</ResourceDictionary>