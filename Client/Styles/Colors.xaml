<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="WindowBackgroundColor">#1E1E1E</Color>
    <SolidColorBrush x:Key="WindowBackgroundColorBrush" Color="{StaticResource WindowBackgroundColor}"></SolidColorBrush>

    <Color x:Key="ButtonColor">#A0A5EB</Color>
    <SolidColorBrush x:Key="ButtonColorBrush" Color="{StaticResource ButtonColor}"></SolidColorBrush>

    <Color x:Key="WrapperColor">#212121</Color>
    <SolidColorBrush x:Key="WrapperColorBrush" Color="{StaticResource WrapperColor}"></SolidColorBrush>

    <Color x:Key="ContextMenuColor">#303030</Color>
    <SolidColorBrush x:Key="ContextMenuColorBrush" Color="{StaticResource ContextMenuColor}"></SolidColorBrush>

    <Color x:Key="UsersListBackgroundColor">#252525</Color>
    <SolidColorBrush x:Key="UsersListBackgroundColorBrush" Color="{StaticResource UsersListBackgroundColor}"></SolidColorBrush>

    <Color x:Key="BorderColor">#5E6175</Color>
    <SolidColorBrush x:Key="BorderBrushColor" Color="{StaticResource BorderColor}"></SolidColorBrush>


</ResourceDictionary>